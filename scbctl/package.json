{
  "name": "scbctl",
  "description": "secureCodeBox CLI",
  "version": "0.1.0",
  "author": "iteratec GmbH",
  "bin": {
    "scbctl": "./bin/run"
  },
  "bugs": "https://github.com/secureCodeBox/scbctl/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "axios": "^0.19.2",
    "chalk": "^2.4.2",
    "execa": "^2.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-security": "^1.4.0",
    "globby": "^11.0.0",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/secureCodeBox/scbctl",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "scbctl",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "secureCodeBox/scbctl",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "lint": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
