{
  "env": {
    "node": true,
    "commonjs": true,
    "jest": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "extends": ["eslint:recommended", "plugin:security/recommended"],
  "plugins": ["prettier", "security"],
  "rules": {
    "security/detect-object-injection": "off",
    "no-console": "off"
  }
}
